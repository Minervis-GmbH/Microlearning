{
  "name": "minervis/togo",
  "description": " A fork of the Project srtile by studeraimann",
  "type": "project",
  "keywords": [
    "ILIAS",
    "Plugin"
  ],
  "homepage": "https://github.com/Minervis-GmbH/Microlearning",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "minervis GMbH",
      "email": "ilias-service@minervis.com",
      "homepage": "https://minervis.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/Minervis-GmbH/Microlearning/issues"
  },
  "require": {
    "php": ">=7.0",
    "components/flag-icon-css": "^3.4",
    "ksubileau/color-thief-php": "^1.4",
    "srag/activerecordconfig": ">=0.1.0",
    "srag/custominputguis": ">=0.1.0",
    "srag/dic": ">=0.1.0",
    "srag/librariesnamespacechanger": ">=0.1.0",
    "srag/notifications4plugin": ">=0.1.0",
    "srag/removeplugindataconfirm": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\Plugins\\SrTile\\": "src/"
    },
    "classmap": [
      "classes/"
    ]
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "pre-autoload-dump": [
      "srag\\LibrariesNamespaceChanger\\LibrariesNamespaceChanger::rewriteLibrariesNamespaces",
      "srag\\LibrariesNamespaceChanger\\PHP72Backport::PHP72Backport"
    ]
  }
}
